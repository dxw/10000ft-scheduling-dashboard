%div.screen
  %div.projects
    -# %div.project.oncall
    -# %div.project.holiday
    -# %div.project{ class: 'project-color' }
    - @projects.each do |project|
      %div.project.unassigned
        %h2= project.name
        %ul.team-members
          - project.users.each do |user|
            %li
              %div
                = image_tag(user.thumbnail, class: 'img')
                %h3.first-name= user.first_name
